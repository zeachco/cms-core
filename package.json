{
  "name": "cms-core",
  "version": "2.2.2",
  "private": false,
  "main": "src/index.js",
  "description": "core functionnalities",
  "author": "Olivier Rousseau-Guyot",
  "repository": {
    "type": "git",
    "url": "http://github.com/zeachco/cms-core.git"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "enzyme": "^3.0.0",
    "jsx-test-helpers": "^3.0.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "auto-bind-es5": "^1.1.0",
    "webpack-production-setup": "^4.1.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "npm t && webpack",
    "preversion": "npm t",
    "postversion": "git push --follow-tags ",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js",
      "!build/**/*"
    ],
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  }
}
